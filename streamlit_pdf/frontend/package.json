{
  "name": "pdf-viewer",
  "license": "Apache-2.0",
  "version": "1.0.8",
  "private": true,
  "type": "module",
  "dependencies": {
    "@streamlit/component-v2-lib": "^0.1.0",
    "@tanstack/react-virtual": "^3.13.12",
    "pdfjs-dist": "5.4.296",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^10.2.0"
  },
  "scripts": {
    "build": "npm run clean && npm run typecheck && cross-env NODE_ENV=production vite build",
    "clean": "rimraf build",
    "dev": "cross-env NODE_ENV=development vite build --watch",
    "format": "prettier --write src/**/*.{ts,tsx,css} vite.config.ts",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.1.0",
    "jsdom": "^26.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-static-copy": "^3.1.0",
    "vitest": "^3.2.4"
  }
}
